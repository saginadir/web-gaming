  "license": "MIT",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "react": "^15.0.2",
    "flux": "^2.1.1",
    "jquery": "^2.2.3",
    "bootstrap": "^3.3.6",
    "lodash": "^4.12.0"
  },

  "overrides": {
    "bootstrap": {
      "main": [
        "less/mixins/*",
        "less/!(variables).less",
        "dist/fonts/*",
        "dist/js/*",
        "dist/css/*"
      ]
    },

    "flux": {
      "main": [
        "dist/*"
      ]
    },

    "jquery": {
      "main": [
        "dist/*"
      ]
    },

    "lodash": {
      "main": [
        "dist/*"
      ]
    },

    "react": {
      "main": [
        "*.js"
      ]
    }
  }

}
